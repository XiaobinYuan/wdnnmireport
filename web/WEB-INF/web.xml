<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
		  http://java.sun.com/xml/ns/javaee/web-app_4_0.xsd"
           version="4.0">

    <servlet>
        <servlet-name>reportEngine</servlet-name>
        <servlet-class>mf.nps.ReportEngine</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>reportEngine</servlet-name>
        <url-pattern>/reportEngine</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>initMap</servlet-name>
        <servlet-class>mf.nps.ThroughputServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>initMap</servlet-name>
        <url-pattern>/initMap</url-pattern>
    </servlet-mapping>


    <servlet>
        <servlet-name>throughput</servlet-name>
        <servlet-class>mf.nps.ThroughputServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>throughput</servlet-name>
        <url-pattern>/throughput</url-pattern>
    </servlet-mapping>
</web-app>
